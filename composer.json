{
    "name": "nekman/es-pagination",
    "description": "Deep pagination for the Elasticsearch client",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Nekman\\EsPagination\\": "src"
        }
    },
    "require": {
        "php": "^7.4||^8.0",
        "elasticsearch/elasticsearch": "^7.11||^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix . --rules=@PSR2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "replace": {
        "nekman/es-pagination": "0.0.5"
    }
}
